# Environment Variables Template
# Copy this file to .env and fill with your values

# Secret key for Flask sessions
SECRET_KEY=your-secret-key-here

# Database URL
DATABASE_URL=sqlite:///kis_couture.db

# Upload folder
UPLOAD_FOLDER=uploads

# Master Recovery Credentials (KEEP SECRET - NEVER COMMIT)
MASTER_USERNAME=admin_master
MASTER_PASSWORD=ChangeThisToAStrongPassword123!

# Port (optional)
PORT=5000
